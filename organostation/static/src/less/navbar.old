/*
Navigation Styling elements 
*/
.navbar {
    font-family: Montserrat, sans-serif;
    margin-bottom: 0;
    background-color: @black;
    border: 0;
    font-size: 11px !important;
    letter-spacing: 4px;
    opacity: 0.6;
}

.navbar {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #fff;
    height: 50px;
}

.navbar li {
    display: inline-block;
}